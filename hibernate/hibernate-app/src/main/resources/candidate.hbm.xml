<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.techlabs.candidate.Candidate" table="candidate">
               <id name="id" type="int" column="id" >
               <generator class="assigned"/>           
              </id>
              <property name="name" >
                         <column name="name" />
              </property>
              <property name="cgpa" >
                        <column name="cgpa"/>
              </property>
              </class>
</hibernate-mapping>